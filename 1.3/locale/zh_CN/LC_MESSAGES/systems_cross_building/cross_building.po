"$ sudo pip install conan --upgrade\n"
"$ cd project\n"
"\n"
"$ conan create . user/channel --build missing\n"
"$ conan remote add myremoteARMV7 http://some.remote.url\n"
"$ conan upload \"*\" -r myremoteARMV7 --all"
