"standalone_toolchain=/myfolder/arm_21_toolchain\n"
"target_host=arm-linux-androideabi\n"
"cc_compiler=gcc\n"
"cxx_compiler=g++\n"
"\n"
"[settings]\n"
"compiler=gcc\n"
"compiler.version=4.9\n"
