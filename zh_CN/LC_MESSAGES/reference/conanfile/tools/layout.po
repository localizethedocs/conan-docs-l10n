msgid "Usage:"
msgstr ""

#: ../../../reference/conanfile/tools/layout.rst:38
msgid ""
"from conans import ConanFile\n"
"from conan.tools.layout import LayoutPackager\n"
"\n"
"class Pkg(ConanFile):\n"
"\n"
"    def layout(self):\n"
"        ...\n"
"\n"
"    def package(self):\n"
"        LayoutPackager(self).package()"
msgstr ""
