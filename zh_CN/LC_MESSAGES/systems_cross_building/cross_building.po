"CC=$target_host-$cc_compiler\n"
"CXX=$target_host-$cxx_compiler\n"
"LD=$target_host-ld\n"
"STRIP=$target_host-strip\n"
"CFLAGS= -fPIC -I$standalone_toolchain/include/c++/4.9.x\n"
"CXXFLAGS= -fPIC -I$standalone_toolchain/include/c++/4.9.x\n"
"LDFLAGS="
msgstr ""

