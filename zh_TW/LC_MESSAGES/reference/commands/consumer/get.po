#: ../../../reference/commands/consumer/get.rst:5
msgid "conan get"
msgstr ""

#: ../../../reference/commands/consumer/get.rst:7
msgid "$ conan get [-h] [-p PACKAGE] [-r REMOTE] [-raw] reference [path]"
msgstr ""

#: ../../../reference/commands/consumer/get.rst:11
msgid "Gets a file or list a directory of a given reference or package."
msgstr ""

#: ../../../reference/commands/consumer/get.rst:13
msgid ""
"positional arguments:\n"
"  reference             package recipe reference\n"
"  path                  Path to the file or directory. If not specified "
"will\n"
"                        get the conanfile if only a reference is specified "
"and\n"
"                        a conaninfo.txt file contents if the package is "
"also\n"
"                        specified\n"
"\n"
"optional arguments:\n"
"  -h, --help            show this help message and exit\n"
"  -p PACKAGE, --package PACKAGE\n"
"                        Package ID\n"
"  -r REMOTE, --remote REMOTE\n"
"                        Get from this specific remote\n"
"  -raw, --raw           Do not decorate the text"
msgstr ""

#: ../../../reference/commands/consumer/get.rst:31
msgid "**Examples**:"
msgstr ""

#: ../../../reference/commands/consumer/get.rst:33
msgid "Print the conanfile.py from a remote package:"
msgstr ""

#: ../../../reference/commands/consumer/get.rst:35
msgid "$ conan get zlib/1.2.8@conan/stable -r conan-center"
msgstr ""

#: ../../../reference/commands/consumer/get.rst:40
msgid "List the files for a local package recipe:"
msgstr ""

#: ../../../reference/commands/consumer/get.rst:42
msgid ""
"$ conan get zlib/1.2.11@conan/stable .\n"
"\n"
"Listing directory '.':\n"
" CMakeLists.txt\n"
" conanfile.py\n"
" conanmanifest.txt"
msgstr ""

#: ../../../reference/commands/consumer/get.rst:51
msgid "Print a file from a recipe folder:"
msgstr ""

#: ../../../reference/commands/consumer/get.rst:53
msgid "$ conan get zlib/1.2.11@conan/stable conanmanifest.txt"
msgstr ""

#: ../../../reference/commands/consumer/get.rst:57
msgid "Print the conaninfo.txt file for a binary package:"
msgstr ""

#: ../../../reference/commands/consumer/get.rst:59
msgid ""
"$ conan get zlib/1.2.11@conan/stable -p "
"09512ff863f37e98ed748eadd9c6df3e4ea424a8"
msgstr ""

#: ../../../reference/commands/consumer/get.rst:63
msgid ""
"[settings]\n"
"    arch=x86_64\n"
"    build_type=Release\n"
"    compiler=apple-clang\n"
"    compiler.version=8.1\n"
"    os=Macos\n"
"\n"
"[requires]\n"
"\n"
"\n"
"[options]\n"
"    # ..."
msgstr ""

#: ../../../reference/commands/consumer/get.rst:78
msgid "List the files from a binary package in a remote:"
msgstr ""

#: ../../../reference/commands/consumer/get.rst:80
msgid ""
"$ conan get zlib/1.2.11@conan/stable . -p "
"09512ff863f37e98ed748eadd9c6df3e4ea424a8 -r conan-center\n"
"\n"
"Listing directory '.':\n"
" conan_package.tgz\n"
" conaninfo.txt\n"
" conanmanifest.txt"
msgstr ""
